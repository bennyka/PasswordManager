<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.fh.passwordmanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_locked"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.fh.passwordmanager.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.ListsActivity"></activity>
        <activity android:name=".activites.ViewEntryActivity"></activity>
        <activity android:name=".activites.EditEntryActivity"></activity>
        <activity android:name="SetMasterPasswordActivity"></activity>
    </application>

</manifest>
